{
  "name": "spacex",
  "version": "1.0.0",
  "homepage": "https://gitname.github.io/spacex_graphQL_apollo/",
  "description": "App that lists SpaceX launch missions",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
